{
  "name": "theway-instalink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn check-env",
    "dev": "next dev",
    "dev:https": "concurrently \"yarn dev -p 3002\" \"local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3000 --target 3002\"",
    "prebuild": "yarn check-env",
    "build": "next build",
    "start": "next start",
    "check-env": "node ./scripts/check-env.js",
    "dc:up": "docker-compose up -d",
    "dc:stop:": "docker-compose stop",
    "dc:down": "docker-compose down"
  },
  "dependencies": {
    "@tryghost/content-api": "^1.4.11",
    "aws-sdk": "^2.846.0",
    "bootstrap-reboot": "^4.5.4",
    "dedent-tabs": "^0.8.2",
    "file-type": "^16.2.0",
    "http-status-codes": "^2.1.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mailgun-js": "^0.22.0",
    "mongodb": "^3.6.3",
    "nanoid": "^3.1.20",
    "next": "^13.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superstruct": "^0.12.1",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mailgun-js": "^0.22.11",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.7",
    "@types/node-mailjet": "^3.3.4",
    "@types/react": "^16.9.56",
    "@types/tryghost__content-api": "^1.3.5",
    "@types/use-persisted-state": "^0.3.0",
    "concurrently": "^7.6.0",
    "dotenv": "^8.2.0",
    "local-ssl-proxy": "^1.3.0",
    "sass": "^1.29.0",
    "typescript": "^4.9.3"
  }
}
